			function getarea(val) {
				var url = 'home.php?mod=spacecp&ac=apply&op=caddie&do=area&val='+val;
				jQuery.post(url, {}, function(msg) {
					jQuery('#field').html(msg);
					dojo.attr('field','value',dojo.attr('hidden_club_place_info2_of_caddie','value'));
				});
			}

			function ffield(fvalue){
				var ty=jQuery("#"+fvalue).val();
				jQuery.post('home.php?mod=spacecp&ac=usefiled&op=aj',{pid:ty},function(msg){
					jQuery("#"+fvalue+"a").html(msg);
					if(fvalue=='one'){
						dojo.attr('onea','value',dojo.attr('hidden_caddie_working_place_info1_2','value'));
					}
					if(fvalue=='two'){
						dojo.attr('twoa','value',dojo.attr('hidden_caddie_working_place_info2_2','value'));
					}
					if(fvalue=="three"){
						dojo.attr('threea','value',dojo.attr('hidden_caddie_working_place_info3_2','value'));
					}	
				});
				
			}

			function init_club_place_info(){
				
				var hidden_club_place_info1_of_caddie = dojo.attr('hidden_club_place_info1_of_caddie','value');
				dojo.attr('province','value',hidden_club_place_info1_of_caddie);
				getarea(hidden_club_place_info1_of_caddie);

				dijit.byId('caddie_beginning_working_date').set('displayedValue',dojo.attr('hidden_caddie_beginning_working_date','value'));
				
				dijit.byId('caddie_birth_date').set('displayedValue',dojo.attr('hidden_caddie_birth_date','value'));

				var hidden_caddie_working_place_info1_1 = dojo.attr('hidden_caddie_working_place_info1_1','value');
				dojo.attr('one','value',hidden_caddie_working_place_info1_1);
